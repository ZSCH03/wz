var picCaptcha={};picCaptcha.init=function(a,b){new Render2().get(function(c,e){var d=nyonline.md5(c).toString();picCaptcha.deviceFinger=d;picCaptcha.vidSign=a;b&&b()})};picCaptcha.getUrl=function(b,e){var c=new Date().getTime();var a=nyonline.md5(picCaptcha.vidSign+picCaptcha.deviceFinger+b+c).toString();var d={sn:a,uid:"",did:picCaptcha.deviceFinger,chl:b,tt:c};nyonline.jsonp("/getPicSign2.c",d,function(f){switch(f.code){case 0:e&&e(f.url);break;default:MsgBox.tips("提示","请刷新页面重试"+f.code);break}},function(f){})};