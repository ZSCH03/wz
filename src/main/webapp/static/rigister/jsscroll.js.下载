function scrollbar(b,h,f,e,c){var d=document.getElementById(b);var g=document.getElementById(h);var i=document.getElementById(f);var a=document.getElementById(e);var k=document.getElementById(c);g.style.height=33+"px";g.onmousedown=function(o){var m=o||event;console.log(m.clientY);var n=m.clientY-g.offsetTop;document.onmousemove=function(r){var p=r||event;var q=p.clientY-n;l(q)};document.onmouseup=function(){document.onmousemove=null;document.onmouseup=null;g.releaseCapture&&g.releaseCapture()};g.setCapture&&g.setCapture();return false};function l(m){m<0&&(m=0);m>i.offsetHeight-g.offsetHeight&&(m=i.offsetHeight-g.offsetHeight);g.style.top=m+"px";var n=m/(i.offsetHeight-g.offsetHeight);a.style.top=-(a.offsetHeight-k.offsetHeight)*n+"px"}function j(o,n){function m(r){var q=r||event;var p=false;if(q.wheelDelta){if(q.wheelDelta<0){p=true}else{p=false}}else{if(q.detail>0){p=true}else{p=false}}n&&n(p);q.preventDefault&&q.preventDefault();return false}if(window.navigator.userAgent.toLowerCase().indexOf("firefox")!=-1){o.addEventListener("DOMMouseScroll",m,false)}else{o.onmousewheel=m}}j(d,function(m){var n=g.offsetTop;if(m){n+=10}else{n-=10}l(n)})};