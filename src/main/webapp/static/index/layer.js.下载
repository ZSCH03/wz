var layer={appendLayer:function(a){$("body").append(a);var b=function(c){c.preventDefault()};document.body.style.overflow="hidden";$("[action-type=close]").on("click",function(){$("[node-type=W-layer]").remove();lottery.saveCloseStatusInCookies($(this).attr("id"),null);document.body.style.overflow="";return false});$("[node-type=W-layer] .main").on("touchmove",function(){event.stopPropagation()})},getLayer:function(c,a,b){arguments.length===3?this.btnTxt=b:this.btnTxt="立即抽奖";this.layerA='<div class="layer" node-type="W-layer" id="layer_lottery"><div id="suc" class="win wincont banner_bg"><div class="main">'+a+'</div><div class="layer_btn"><a href="JavaScript:lottery.gotoLotteryPage(1)" class="">'+this.btnTxt+"</a></div></div>";layer.appendLayer(this.layerA)}};