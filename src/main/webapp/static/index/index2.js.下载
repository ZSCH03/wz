var _Index={};_Index.init=function(){var d=$("#banner");var a=$("#banner>.w256");var c=$("#banner>.w256 img");var b=0;if(window.innerWidth){b=window.innerWidth}else{if((document.body)&&(document.body.clientWidth)){b=document.body.clientWidth}}if(b<=1000){a.css({width:"2560px",height:"320px","margin-left":"-1280px"});c.css("width","2560px")}};var Class={create:function(){return function(){this.initialize.apply(this,arguments)}}};Function.prototype.bind=function(a){var b=this;return function(){b.apply(a,arguments)}};var Scroll=Class.create();Scroll.prototype={initialize:function(c,a,b){this.element=$(c)[0];this.element.innerHTML+=this.element.innerHTML;this.height=a;this.delay=b*5000;this.maxHeight=this.element.scrollHeight/2;this.counter=0;this.scroll();this.timer="";this.element.onmouseover=this.stop.bind(this);this.element.onmouseout=function(){this.timer=setTimeout(this.scroll.bind(this),5000)}.bind(this)},scroll:function(){if(this.element.scrollTop<this.maxHeight){this.element.scrollTop=this.element.scrollTop+1;this.counter++}else{this.element.scrollTop=0;this.counter=0}if(this.counter<this.height){this.timer=setTimeout(this.scroll.bind(this),5)}else{this.counter=0;this.timer=setTimeout(this.scroll.bind(this),this.delay)}},stop:function(){clearTimeout(this.timer)}};_Index.showLogin=function(a){if(a==1){topLogin()}};$(function(){if($("#announcement li").length>=2){setTimeout(function(){new Scroll("#announcement",28,1)},5000)}_Index.banner();$(".fixed-bar .top").click(function(){$("html,body").animate({scrollTop:"0px"},200)})});_Index.index=0;_Index.bannerNum=$(".w256 img").length;_Index.picTimer=null;_Index.speed=500;_Index.gotoBan=function(a){$(".w256 a").eq(a).fadeIn(_Index.speed).siblings(".w256 a").fadeOut(_Index.speed);_Index.index++;if(_Index.index==_Index.bannerNum){_Index.index=0}clearInterval(_Index.picTimer);_Index.picTimer=setInterval(function(){_Index.show(_Index.index);_Index.index++;if(_Index.index==_Index.bannerNum){_Index.index=0}},5000)};_Index.banner=function(){var b=$(".bannav em");var a=$(".w256 a");if(_Index.bannerNum>1){$(".w256 a").eq(0).show().siblings(".w256 a").hide()}$(".bannav em").click(function(){$(this).addClass("cur").siblings("em").removeClass("cur");var c=$(".bannav em").index(this);$(".w256 a").eq(c).fadeIn(_Index.speed).siblings(".w256 a").fadeOut(_Index.speed);_Index.index=c++;if(_Index.index==_Index.bannerNum){_Index.index=0}clearInterval(_Index.picTimer);_Index.picTimer=setInterval(function(){_Index.show(_Index.index);_Index.index++;if(_Index.index==_Index.bannerNum){_Index.index=0}},5000)});_Index.picTimer=setInterval(function(){_Index.show(_Index.index);_Index.index++;if(_Index.index==_Index.bannerNum){_Index.index=0}},5000);_Index.show=function(c){$(".bannav em").removeClass("cur").eq(c).addClass("cur");$(".w256 a").eq(c).fadeIn(_Index.speed).siblings(".w256 a").fadeOut(_Index.speed)}};_Index.goleft=function(){_Index.index=_Index.index-2;if(_Index.index<0){_Index.index=_Index.bannerNum+_Index.index}_Index.gotoBan(_Index.index)};_Index.goright=function(){_Index.gotoBan(_Index.index)};_Index.gotoList=function(b){var a="";if(b=="sz"){a="/packlist/"}else{if(b=="qnb"){a="/packqnb/"}else{if(b=="dyb"){a="/dyb/"}else{if(b=="jxzq"){a="/bidlist/"}}}}window.location.href=client(a)};_Index.gotoBid=function(b,c){var a="";if(c==1){a="/packlist/"}else{if(c==2){a="/packqnb/"}else{if(c==3){a="/dyb/"}else{if(c==4){a="/bidlist/"}}}}window.location.href=client(a)+b+".html"};_Index.newareabid=function(a,b){if(b){_Index.gotoBid(a,1)}else{topLogin()}};